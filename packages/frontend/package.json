{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./index.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run",
    "codegen:playwright": "playwright codegen",
    "test:playwright": "npx playwright test --project=playwright",
    "test:accessibility": "npx playwright test --project=accessibility",
    "install:browsers": "playwright install",
    "test:watch": "vitest watch",
    "build:docker": "docker build -f ./Dockerfile -t frontend ../..",
    "run:docker": "docker run -it -p 80:80 frontend",
    "i18n:check": "tsx ./src/i18n/check.ts",
    "i18n:sort": "tsx ./src/i18n/check.ts --sort"
  },
  "dependencies": {
    "@altinn/altinn-components": "^0.10.4",
    "@digdir/designsystemet-css": "0.11.0-next.10",
    "@digdir/designsystemet-react": "1.0.0-next.15",
    "@digdir/designsystemet-theme": "1.0.0-next.14",
    "@microsoft/applicationinsights-react-js": "17.3.2",
    "@microsoft/applicationinsights-web": "3.3.3",
    "@navikt/aksel-icons": "^6.17.0",
    "@tanstack/react-query": "^5.62.11",
    "bff-types-generated": "workspace:*",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "embeddable-markdown-html": "workspace:*",
    "graphql": "^16.10.0",
    "graphql-request": "^6.1.0",
    "i18next": "^23.16.8",
    "i18next-icu": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.2.0",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^6.28.1",
    "sse.js": "^2.5.0",
    "use-debounce": "^10.0.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@playwright/test": "^1.49.1",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^20.17.10",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "dotenv": "^16.4.7",
    "jsdom": "^24.1.3",
    "msw": "^2.7.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vitest": "^2.1.8"
  },
  "msw": {
    "workerDirectory": ["public"]
  }
}
